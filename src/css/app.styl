.sidenav {
  padding-top: 0px !important;
  height: 100%;
  width: 40px;
  position: absolute;
  z-index: 1;
  right: 0px;
  background-color: #ffffff00;
  overflow-x: hidden;
  transition: 0.5s;
  padding-top: 60px;
}
.SiteNav-Menu {
  box-shadow: 0 3px 6px 0 rgba(0, 0, 0, 0.16);
  background: #ffffff;
  margin-left: 40px;
  height: 100%;
}
.SiteNav_Header {
  height: 26px;
  font-family: HelveticaNeue;
  font-size: 16px;
  font-weight: bold;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.23;
