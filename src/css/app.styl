  text-align: left;
  color: #606060;
}
.sidenav a {
  padding: 8px 8px 8px 32px;
  text-decoration: none;
  font-size: 25px;
  color: #818181;
  display: block;
  transition: 0.3s;
}
.SideNav-Code-Group {
  cursor: pointer;
  position: absolute;
  left: 0px;
  writing-mode: tb-rl;
  width: 25px;
  display: inline;
  font-family: HelveticaNeue;
  font-size: 12px;
  font-weight: 500;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.23;
  letter-spacing: normal;
  text-align: left;
  color: #606060;
  padding: .2rem;
  border-radius: 6px;
  background-color: #e2efb9;
}
.sidenav a:hover {
  color: #f1f1f1;
}


